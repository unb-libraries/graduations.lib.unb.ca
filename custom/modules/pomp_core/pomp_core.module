<?php

/**
 * Implements hook_form_alter().
 */
function pomp_core_form_alter(&$form, &$form_state, $form_id) {

  if ($form_id == "views_exposed_form" and $form['#id'] == 'views-exposed-form-search-page-1') {
    kint($form['#id']);
    $form_state->setValidationEnforced(FALSE);
    $form_state->setValidationComplete(TRUE);
  }
}
