{
  "authors": [
    {
      "name": "UNB Libraries libsupport@unb.ca",
      "role": "Creator"
    }
  ],
  "autoload": {
    "classmap": [
      "scripts/composer/ScriptHandler.php"
    ]
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "cweagans/composer-patches": true,
      "drupal/core-composer-scaffold": true
    },
    "discard-changes": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "description": "Drupal 8 component of graduations.lib.unb.ca",
  "extra": {
    "enable-patching": true,
    "installer-paths": {
      "core": [
        "type:drupal-core"
      ],
      "drush/contrib/{$name}": [
        "type:drupal-drush"
      ],
      "libraries/{$name}": [
        "type:drupal-library"
      ],
      "modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "themes/contrib/{$name}": [
        "type:drupal-theme"
      ]
    },
    "patches": {}
  },
  "license": "MIT",
  "minimum-stability": "dev",
  "name": "unb-libraries/graduations.lib.unb.ca",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "package": {
        "dist": {
          "type": "file",
          "url": "https://malsup.github.io/jquery.cycle.all.js"
        },
        "name": "malsup/jquery.cycle",
        "require": {
          "composer/installers": "^1.0.20"
        },
        "type": "drupal-library",
        "version": "1.0"
      },
      "type": "package"
    },
    {
      "package": {
        "dist": {
          "type": "zip",
          "url": "https://github.com/malsup/cycle/archive/3.0.3.zip"
        },
        "extra": {
          "installer-name": "jquery.cycle"
        },
        "name": "jquery/cycle",
        "type": "drupal-library",
        "version": "3.0.3"
      },
      "type": "package"
    },
    {
      "package": {
        "dist": {
          "type": "zip",
          "url": "https://github.com/briancherne/jquery-hoverIntent/archive/v1.10.0.zip"
        },
        "extra": {
          "installer-name": "jquery.hoverIntent"
        },
        "name": "jquery/hover_intent",
        "type": "drupal-library",
        "version": "1.10.0"
      },
      "type": "package"
    },
    {
      "package": {
        "dist": {
          "type": "zip",
          "url": "https://github.com/douglascrockford/JSON-js/archive/master.zip"
        },
        "extra": {
          "installer-name": "json2"
        },
        "name": "jquery/json2",
        "type": "drupal-library",
        "version": "dev-master"
      },
      "type": "package"
    },
    {
      "package": {
        "dist": {
          "type": "zip",
          "url": "https://github.com/tobia/Pause/archive/master.zip"
        },
        "extra": {
          "installer-name": "jquery.pause"
        },
        "name": "jquery/pause",
        "type": "drupal-library",
        "version": "dev-master"
      },
      "type": "package"
    },
    {
      "package": {
        "dist": {
          "type": "zip",
          "url": "https://github.com/FortAwesome/Font-Awesome/archive/5.14.0.zip"
        },
        "extra": {
          "installer-name": "fontawesome"
        },
        "name": "unb-libraries/font_awesome",
        "type": "drupal-library",
        "version": "5.14.0"
      },
      "type": "package"
    },
    {
      "package": {
        "name": "drupal/login_return_page",
        "source": {
          "reference": "8.x-1.x",
          "type": "git",
          "url": "https://git.drupalcode.org/sandbox/gnuget-2824414.git"
        },
        "type": "drupal-module",
        "version": "1.x-dev"
      },
      "type": "package"
    }
  ],
  "require": {
    "drupal/admin_toolbar": "3.3.0",
    "drupal/bootstrap": "3.25",
    "drupal/bootstrap4": "2.1.16",
    "drupal/core": "9.5.5",
    "drupal/fontawesome": "2.25",
    "drupal/google_analytics": "2.5",
    "drupal/health_check": "1.3",
    "drupal/inline_entity_form": "1.0-rc15",
    "drupal/login_return_page": "1.x-dev",
    "drupal/node_edit_protection": "1.1",
    "drupal/pathauto": "1.11",
    "drupal/redirect_after_login": "2.7",
    "drupal/redis": "1.6",
    "drupal/samlauth": "3.8",
    "drupal/search_api": "1.28",
    "drupal/search_api_solr": "4.1.0",
    "drupal/token_filter": "1.4",
    "drupal/views_slideshow": "4.8",
    "jquery/cycle": "3.0.3",
    "jquery/hover_intent": "1.10.0",
    "jquery/json2": "dev-master",
    "jquery/pause": "dev-master",
    "malsup/jquery.cycle": "1.0",
    "oomphinc/composer-installers-extender": "^2",
    "unb-libraries/drupal-builder": "^9",
    "unb-libraries/saml_features": "dev-9.x-1.x",
    "unb-libraries/unb_lib_zircon": "dev-9.x-bootstrap-4.x"
  },
  "require-dev": {
    "unb-libraries/drupal-development-libraries": "^9"
  },
  "scripts": {
    "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
    "post-install-cmd": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ],
    "post-update-cmd": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ],
    "pre-install-cmd": [
      "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
    ],
    "pre-update-cmd": [
      "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
    ]
  },
  "type": "project"
}
