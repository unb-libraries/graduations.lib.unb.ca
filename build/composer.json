{
    "authors": [
        {
            "name": "UNB Libraries libsupport@unb.ca",
            "role": "Creator"
        }
    ],
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "config": {
        "preferred-install": "source"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "description": "Project definition for honorary.lib.unb.ca",
    "extra": {
        "enable-patching": true,
        "installer-paths": {
            "core": [
                "type:drupal-core"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "libraries/{$name}": [
                "type:drupal-library"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "themes/contrib/{$name}": [
                "type:drupal-theme"
            ]
        },
        "patches": {
            "drupal/core": {
                "Allow for no-link menu items": "https://www.drupal.org/files/issues/allow-menu-items-without-path-1543750-124.patch",
                "Allow for titles on exposed block filters": "https://www.drupal.org/files/issues/2720101-24-views-exposed-block-title-test.diff"
            },
            "drupal/zircon": {
                "Correct monokai typo": "https://www.drupal.org/files/issues/monokai_css-2656702-2.patch"
            }
        }
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "name": "unblibraries/honorary.lib.unb.ca",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "package": {
                "dist": {
                    "type": "file",
                    "url": "https://malsup.github.io/jquery.cycle.all.js"
                },
                "name": "malsup/jquery.cycle",
                "require": {
                    "composer/installers": "^1.0.20"
                },
                "type": "drupal-library",
                "version": "master"
            },
            "type": "package"
        }
    ],
    "require": {
        "composer/installers": "^1.2.0",
        "cweagans/composer-patches": "~1.6.1",
        "drupal-composer/drupal-scaffold": "~2.2.0",
        "drupal/config_update": "1.1",
        "drupal/console": "~1.0",
        "drupal/core": "8.3.1",
        "drupal/externalauth": "1.0",
        "drupal/features": "3.0-beta8",
        "drupal/field_collection": "1.0-alpha1",
        "drupal/google_analytics": "~2.1",
        "drupal/ldap": "~3.0-beta1",
        "drupal/pathauto": "~1.0-rc1",
        "drupal/search_api": "~1.0-beta4",
        "drupal/token_filter": "1.0-beta1",
        "drupal/views_slideshow": "4.4",
        "drupal/zircon": "1.0-rc1",
        "drush/drush": "~8.1",
        "malsup/jquery.cycle": "master",
        "oomphinc/composer-installers-extender": "1.1.1",
        "webflo/drupal-finder": "^0.2.1",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "behat/mink": "~1.7",
        "behat/mink-goutte-driver": "~1.2",
        "drupal/devel": "1.0-alpha1",
        "drupal/drupal-extension": "~3.0",
        "jcalderonzumba/gastonjs": "~1.0.2",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "mikey179/vfsStream": "~1.2",
        "phpunit/phpunit": "~4.8",
        "symfony/css-selector": "~2.8"
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ]
    },
    "type": "project"
}
