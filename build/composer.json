{
  "authors": [
    {
      "name": "UNB Libraries libsupport@unb.ca",
      "role": "Creator"
    }
  ],
  "autoload": {
    "classmap": [
      "scripts/composer/ScriptHandler.php"
    ]
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "cweagans/composer-patches": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "drupal/core-composer-scaffold": true,
      "oomphinc/composer-installers-extender": true,
      "phpstan/extension-installer": true
    },
    "discard-changes": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "description": "Drupal 10 component of graduations.lib.unb.ca",
  "extra": {
    "drupal-scaffold": {
      "locations": {
        "web-root": "./"
      }
    },
    "enable-patching": true,
    "installer-paths": {
      "core": [
        "type:drupal-core"
      ],
      "libraries/{$name}": [
        "type:drupal-library"
      ],
      "modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "Commands/contrib/{$name}": [
        "type:drupal-drush"
      ],
      "modules/custom/{$name}": [
        "type:drupal-custom-module"
      ],
      "profiles/custom/{$name}": [
        "type:drupal-custom-profile"
      ],
      "themes/custom/{$name}": [
        "type:drupal-custom-theme"
      ]
    },
    "patches": {}
  },
  "license": "MIT",
  "minimum-stability": "dev",
  "name": "unb-libraries/graduations.lib.unb.ca",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "package": {
        "dist": {
          "type": "file",
          "url": "https://malsup.github.io/jquery.cycle.all.js"
        },
        "name": "malsup/jquery.cycle",
        "require": {
          "composer/installers": "^2"
        },
        "type": "drupal-library",
        "version": "1.0"
      },
      "type": "package"
    },
    {
      "package": {
        "dist": {
          "type": "zip",
          "url": "https://github.com/malsup/cycle/archive/3.0.3.zip"
        },
        "extra": {
          "installer-name": "jquery.cycle"
        },
        "name": "jquery/cycle",
        "type": "drupal-library",
        "version": "3.0.3"
      },
      "type": "package"
    },
    {
      "package": {
        "dist": {
          "type": "zip",
          "url": "https://github.com/briancherne/jquery-hoverIntent/archive/v1.10.0.zip"
        },
        "extra": {
          "installer-name": "jquery.hoverIntent"
        },
        "name": "jquery/hover_intent",
        "type": "drupal-library",
        "version": "1.10.0"
      },
      "type": "package"
    },
    {
      "package": {
        "dist": {
          "type": "zip",
          "url": "https://github.com/douglascrockford/JSON-js/archive/master.zip"
        },
        "extra": {
          "installer-name": "json2"
        },
        "name": "jquery/json2",
        "type": "drupal-library",
        "version": "dev-master"
      },
      "type": "package"
    },
    {
      "package": {
        "dist": {
          "type": "zip",
          "url": "https://github.com/tobia/Pause/archive/master.zip"
        },
        "extra": {
          "installer-name": "jquery.pause"
        },
        "name": "jquery/pause",
        "type": "drupal-library",
        "version": "dev-master"
      },
      "type": "package"
    },
    {
      "package": {
        "dist": {
          "type": "zip",
          "url": "https://github.com/FortAwesome/Font-Awesome/archive/5.14.0.zip"
        },
        "extra": {
          "installer-name": "fontawesome"
        },
        "name": "unb-libraries/font_awesome",
        "type": "drupal-library",
        "version": "5.14.0"
      },
      "type": "package"
    },
    {
      "package": {
        "name": "drupal/login_return_page",
        "source": {
          "reference": "8.x-1.x",
          "type": "git",
          "url": "https://git.drupalcode.org/sandbox/gnuget-2824414.git"
        },
        "type": "drupal-module",
        "version": "1.x-dev"
      },
      "type": "package"
    }
  ],
  "require": {
    "drupal/admin_toolbar": "^3",
    "drupal/bootstrap": "^3",
    "drupal/bootstrap4": "^3",
    "drupal/color": "^1",
    "drupal/core": "^10",
    "drupal/core-composer-scaffold": "^10",
    "drupal/core-recommended": "^10",
    "drupal/fontawesome": "^2",
    "drupal/google_analytics": "^4",
    "drupal/health_check": "^1",
    "drupal/inline_entity_form": "1.0-rc15",
    "drupal/node_edit_protection": "^1",
    "drupal/pathauto": "^1",
    "drupal/quickedit": "^1", 
    "drupal/redirect_after_login": "^2",
    "drupal/redis": "^1",
    "drupal/rdf": "^2", 
    "drupal/samlauth": "^3",
    "drupal/search_api": "^1",
    "drupal/search_api_solr": "^4",
    "drupal/token_filter": "^2",
    "drupal/views_slideshow": "^5",
    "jquery/cycle": "^3",
    "jquery/hover_intent": "^1",
    "jquery/json2": "dev-master",
    "jquery/pause": "dev-master",
    "malsup/jquery.cycle": "^1",
    "oomphinc/composer-installers-extender": "^2",
    "unb-libraries/composer-drupal-scaffold": "^10",
    "unb-libraries/saml_features": "dev-10.x-1.x",
    "unb-libraries/unb_lib_zircon": "dev-10.x-bootstrap-4.x"
  },
  "require-dev": {
    "unb-libraries/drupal-development-libraries": "^10"
  },
  "type": "project"
}
