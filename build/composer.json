{
    "authors": [
        {
            "name": "UNB Libraries libsupport@unb.ca",
            "role": "Creator"
        }
    ],
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        }
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "description": "Project definition for graduations.lib.unb.ca",
    "extra": {
        "enable-patching": true,
        "installer-paths": {
            "core": [
                "type:drupal-core"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "libraries/{$name}": [
                "type:drupal-library"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "themes/contrib/{$name}": [
                "type:drupal-theme"
            ]
        },
        "patches": {
            "drupal/core": {
                "Allow for no-link menu items": "https://www.drupal.org/files/issues/allow-menu-items-without-path-1543750-124.patch"
            },
            "drupal/zircon": {
                "Correct monokai typo": "https://www.drupal.org/files/issues/monokai_css-2656702-2.patch"
            }
        }
    },
    "license": "MIT",
    "minimum-stability": "stable",
    "name": "unblibraries/graduations.lib.unb.ca",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "package": {
                "dist": {
                    "type": "file",
                    "url": "https://malsup.github.io/jquery.cycle.all.js"
                },
                "name": "malsup/jquery.cycle",
                "require": {
                    "composer/installers": "^1.0.20"
                },
                "type": "drupal-library",
                "version": "1.0"
            },
            "type": "package"
        },
        {
            "package": {
                "name": "drupal/login_return_page",
                "version": "1.x-dev",
                "type": "drupal-module",
                "source": {
                    "url": "https://git.drupal.org/sandbox/gnuget/2824414.git",
                    "type": "git",
                    "reference": "8.x-1.x"
                }
            },
            "type": "package"
        },
        {
            "type": "git",
            "url": "https://github.com/unb-libraries/drupal-development-libraries.git"
        }
    ],
    "require": {
        "composer/installers": "^1.2.0",
        "cweagans/composer-patches": "~1.6.1",
        "drupal-composer/drupal-scaffold": "~2.2.0",
        "drupal/config_update": "1.1",
        "drupal/console": "~1.0",
        "drupal/core": "8.4.2",
        "drupal/externalauth": "1.0",
        "drupal/features": "3.0-beta8",
        "drupal/field_collection": "1.0-alpha1",
        "drupal/google_analytics": "2.1",
        "drupal/ldap": "dev-3.x",
        "drupal/login_return_page": "1.x-dev",
        "drupal/pathauto": "1.0",
        "drupal/redis": "1.0-rc2",
        "drupal/search_api": "1.5",
        "drupal/token_filter": "1.0-beta1",
        "drupal/views_slideshow": "4.6",
        "drupal/zircon": "1.0-rc1",
        "drush/drush": "^8.1.15",
        "malsup/jquery.cycle": "1.0",
        "oomphinc/composer-installers-extender": "1.1.1",
        "webflo/drupal-finder": "^0.3.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "unb-libraries/drupal-development-libraries": "^8.0.0"
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ]
    },
    "type": "project"
}
